    "@types/node": "^14.0.0",
    "c8": "^7.0.0",
    "chai": "^4.0.0",
    "chai-spies": "^1.0.0",
    "mocha": "^9.0.0",
    "prettier": "^2.0.0",
    "ts-mocha": "^9.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@civ-clone/core-city": "^0.1.0",
    "@civ-clone/core-city-happiness": "^0.1.0",
    "@civ-clone/core-client": "^0.1.0",
